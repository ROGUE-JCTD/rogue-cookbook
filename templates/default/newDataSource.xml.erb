<dataStore>
    <name><%= @settings['DATABASES']['geonode_imports']['name'] %></name>
    <type>PostGIS</type>
    <enabled>true</enabled>
    <workspace>
    <name>geonode</name>
    <atom:link xmlns:atom="http://www.w3.org/2005/Atom" rel="alternate" href="<%= @settings['OGC_SERVER']['LOCATION']%>rest/workspaces/geonode.xml" type="application/xml"/>
    </workspace>
    <connectionParameters>
        <entry key="Connection timeout">20</entry>
        <entry key="port"><%= @settings['DATABASES']['geonode_imports']['port'] %></entry>
        <entry key="passwd"><%= @data_store_password %></entry>
        <entry key="dbtype">postgis</entry>
        <entry key="host"><%= @settings['DATABASES']['geonode_imports']['host'] %></entry>
        <entry key="validate connections">true</entry>
        <entry key="encode functions">false</entry>
        <entry key="max connections">10</entry>
        <entry key="database"><%= @settings['DATABASES']['geonode_imports']['name'] %></entry>
        <entry key="namespace">http://www.geonode.org/</entry>
        <entry key="schema">public</entry>
        <entry key="Loose bbox">true</entry>
        <entry key="Expose primary keys">false</entry>
        <entry key="fetch size">1000</entry>
        <entry key="Max open prepared statements">50</entry>
        <entry key="preparedStatements">false</entry>
        <entry key="Estimated extends">true</entry>
        <entry key="user"><%= @settings['DATABASES']['geonode_imports']['user'] %></entry>
        <entry key="min connections">1</entry>
    </connectionParameters>
    <__default>false</__default>
</dataStore>